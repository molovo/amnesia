      "Molovo\\Amnesia\\": "src/"
    }
  },
  "require": {
    "PHP": ">=5.5",
    "vlucas/phpdotenv": "2.1.*",
    "molovo/object": "^1.0",
    "predis/predis": "^1.0"
  },
  "suggest": {
    "ext-redis": "To use the built-in Redis driver",
    "ext-memcached": "To use the built-in Memcached driver"
  },
  "require-dev": {
